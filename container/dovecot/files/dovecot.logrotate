/var/log/dovecot/deliver.log /var/log/dovecot/debug.log {
	su dovecot mail
	#delaycompress
	rotate 5
	missingok
	notifempty
	dateext
	#size 10M
	sharedscripts
	postrotate
		/etc/init.d/syslog-ng reload > /dev/null 2>&1 || true
	endscript
}

